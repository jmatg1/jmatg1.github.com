//    Навигация слайдеров
.dots
  display: flex
  width: 300px
  justify-content: start
  align-items: center
  position: relative;
  bottom: 90px;
  left: 6.3%
  position: relative
  li
    flex-basis: 12%
  button
    border-radius: 50%;
    color: #fff
    border-style: solid;
    border-width: 2px;
    border-color: #b6d2e6
    border-radius: 50%;
    background-color: rgb(255, 255, 255);
    width: 24px;
    height: 24px;
    outline none
    cursor: pointer
    &:hover
        background-color: #ccc;
        color: #ccc
  .slick-active:after
    content: ''
    border-radius: 50%;
    background-color: rgb(0, 96, 175);
    opacity: 0.502;
    position: absolute
    width: 10px;
    height: 10px;
    top: 7px
    margin-left: -17px;
  .slick-active button
    border-color: rgb(0, 96, 175);
    opacity: 0.5
