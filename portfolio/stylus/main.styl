      display: block
      position: relative
      margin-top  45px
      margin-bottom: 15px
      flex-basis: 100%;
      justify-content: center;
      text-align: center;
    @media screen and (max-width: 610px)
      border-bottom 1px solid #efefef
      margin-top  10px
      padding-bottom: 10px
    li
      margin-left: 20px;
      @media screen and (max-width: 1160px)
        margin-left: auto
        margin-right: 20px
      @media screen and (max-width: 900px)
        margin-top 5px
        a
          display: block
          height: 25px
          line-height: 25px;
  a:hover
    color: #0004af;
  .search-mobile
    display: none
    @media screen and (max-width: 610px)
      display: block
      color: #000
  .nav-top-right-list
    flex-basis: 30%
    display: flex
    justify-content: flex-end
    align-items center
    margin-right: 20px
    @media screen and (max-width: 1160px)
      margin-right: 0
    li
      margin-left: 20px;
    @media screen and (max-width: 900px)
      display: none;
  .contacts
    flex-basis: 60%
    color: #000
    display: flex;
    justify-content: center;
    align-items: center;
    @media screen and (max-width: 610px)
      position: inherit
      font-size: 15px
      flex-direction: column;

    @media screen and (max-width: 900px) and (min-width: 610px)
      visibility: visible
      display: block
      position: absolute
      right: 5%;
      margin-top: 2%;
      font-size:15px
    @media screen and (max-width: 1078px) and (min-width: 900px)
      flex-basis: 20%;
    .phones
      color: #000
      font-size: 16px
      font-family "Lato Bold",Arial,sans-serif
      @media screen and (max-width: 610px)
        width:130px
        font-size: 15px
      a
        margin-left:25px
        @media screen and (max-width: 610px)
          margin-left:auto
          height: 25px;
          line-height: 25px;
    .time
      position: absolute;
      text-align: center;
      justify-content: center;
      display: flex;
      justify-content: center;
      background: white;
      top: 130px;
      width: 170px;
      color: #949697;
      font-size: 14px;
      font-family: "Lato Regular", Arial, sans-serif;
      @media screen and (max-width: 900px) and (min-width: 610px)
        display: none
      @media screen and (max-width: 610px)
        position: static

//Кнопка открытия меню в мобилке
.toggle-button
  position: absolute
  left: 5%;
  margin-top: 9px;
  @media screen and (max-width: 610px)
    top: 17px;
    left: auto
    right: 5%
  span
    position: relative;
    top: -8px;
    left: 5px;
    font-family: "Lato Regular", Arial, sans-serif;
    color: #0060af;
    font-style: 15px
    @media screen and (max-width: 610px)
      display: none
  @media screen and (min-width: 901px)
    display: none
.container
  display: inline-block
  cursor: pointer

.bar1, .bar2, .bar3
  width: 35px
  height: 2px
  background-color: #0000ff
  margin: 6px 0
  transition: 0.4s
  @media screen and (max-width: 610px)
    background-color: black
    width: 20px
.change .bar1
  -webkit-transform: rotate(-45deg) translate(-9px, 6px)
  transform: rotate(-45deg) translate(-9px, 6px)
.change .bar2
  opacity: 0
.change .bar3
  transform: rotate(45deg) translate(-5px, -3px);
//** END Кнопка открытия меню в мобилке



