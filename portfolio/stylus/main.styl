/*
 *     HEADER
 */
/*= Reset CSS
============= */
html, body {border: 0; margin: 0; padding: 0;}
body {font: 14px "Lato", Arial, sans-serif; min-width: 100%; min-height: 100%; color: #666;}
.container{margin: 0 auto; max-width: 1200px;}
*{margin: 0; padding: 0; box-sizing: border-box;}
.row{float: left; width: 100%; padding: 20px 0 50px;}
//h2{text-align: center; color: #2079df; font-size: 28px; float: left; width: 100%; margin: 30px 0; position: relative; line-height: 58px; font-weight: 400;}
/*= Reset CSS End
================= */

/*= input focus effects css
=========================== */

header.head
  font-family: "Lato Regular", Arial,sans-serif;
  font-size: 14px
  height: 80px
  display: flex
  align-content center
  .wrapper
    display: flex
    justify-content: space-between
    align-items center

  .search-input
    width:190px
    color: #000
    height: 35px
    border 0px
    font-size: inherit
    //margin-left:15px
    &:focus
      outline: none;
    &::placeholder
      color: #293137
  .search
    position: relative;
    //margin-left: 20px
    display: flex;
    flex-basis: 43%;
    margin-left: 22px;
    @media screen and (max-width: 610px)
      display: none

    &:hover .search-input::placeholder
      color #0060af
    &:before
      background:url("../images/search.png") no-repeat;
      content ''
      width: 20px
      height: 20px
      display: inline-block;
      position:relative
      top: 7px;
      //left: 25px;
      z-index: 999;
    &:focus{outline: none;}
    // подчеркивает инпут при активации
    //https://codepen.io/Takumari85/pen/RaYwpJ
    .effect-2{border: 0; padding: 7px 5px; border-bottom: 1px solid #fff;}
    .effect-2 ~ .focus-border{position: absolute; bottom: 0; left: 50%; width: 0; height: 2px; background-color: #3399FF; transition: 0.4s;}
    .effect-2:focus ~ .focus-border{width: 100%; transition: 0.4s; left: 0;}
    .input-effect     {margin-left: 10px;}
  .logo
    display: flex;
    flex-basis: 20%;
    justify-content: center;
    height: 55px;
    @media screen and (max-width:610px )
      height: auto
    img
      @media screen and (max-width:610px )
        width: 115px;
        height: 27px;

.header-menu
   display: flex;
   flex-basis: 45%;
   justify-content: flex-end;
   align-items: center;
   @media screen and (max-width:610px )
      margin-right: 50px;
.nav
    display: flex
    justify-content:  flex-end
    align-items center
    margin-right: 20px
  .nav-item a
    display: flex
    align-items center
    height: 20px
    &:hover
      color #0060af
  .nav-item span
    margin-left: 20px
    margin-right: 8px
@media screen and (max-width: 1160px)
  header.head
    .wrapper
    .search-input
    .search
      margin-left: auto
    .header-menu
      .nav
        margin-right: 0px
        .nav-item
          margin-left: 20px
          &:last-child a
             position: relative
          span
            margin-left: 0
            &.text-cart
              display: none
            &.count
              font-size: 9px;
              font-family: "Lato";
              color: rgb(41, 49, 55);
              line-height: 3;
              position: absolute
              right: -3px;
              height:100%
@media screen and (min-width: 1161px)
  .total-price, .count
    display: none
@media screen and (max-width: 900px)
  .total-price
    display: none
//    end HEADER





.navigation .wrapper
